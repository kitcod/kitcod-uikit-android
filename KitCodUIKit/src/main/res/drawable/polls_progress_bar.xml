<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <solid android:color="@color/kitcodColorWhite" />
            <stroke
                android:width="0.5dp"
                android:color="#46000000"></stroke>
            <corners android:radius="@dimen/_24sdp" />
        </shape>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <solid android:color="#D2DAFD"></solid>
                <!--<gradient
                    android:endColor="@color/topaz"
                    android:startColor="@color/dark_sky_blue" />-->

                <corners android:radius="@dimen/_24sdp" />
            </shape>
        </clip>
    </item>
</layer-list>